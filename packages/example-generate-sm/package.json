{
  "name": "@aloxide/example-generate-sm",
  "description": "Example of using jest with a TS monorepo",
  "version": "0.1.0",
  "author": "Lecle",
  "license": "Apache-2.0 License",
  "repository": {
    "type": "git",
    "url": "git@github.com:lecle/aloxide.git"
  },
  "private": "true",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "prestart": "rimraf ./out",
    "start": "node ./dist/index.js",
