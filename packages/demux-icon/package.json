{
  "name": "@aloxide/demux-icon",
  "description": "Example of using jest with a TS monorepo",
  "version": "1.0.0",
  "license": "ISC",
  "main": "./dist/index",
  "files": [
    "dist"
  ],
  "types": "./dist/*.d.ts",
  "scripts": {
    "start": "ts-node -P ./tsconfig.json src/example.ts | bunyan",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn build",
    "lint": "tslint --project . --fix",
    "test": "jest -o"
  },
  "dependencies": {
    "@aloxide/logger": "^1.0.0",
    "bignumber.js": "^9.0.0",
    "demux": "5.0.2-581",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "bunyan": "^1.8.14"
  }
}
