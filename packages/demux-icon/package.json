{
  "name": "@aloxide/demux-icon",
  "description": "Implement of demux pattern for ICON",
  "version": "0.2.0",
  "author": "Lecle",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lecle/aloxide"
  },
  "main": "./dist/index",
  "files": [
    "dist"
  ],
  "types": "./dist/*.d.ts",
  "scripts": {
    "dev": "ts-node -P ./tsconfig.json src/example.ts | bunyan",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn build",
    "lint": "tslint --project . --fix",
    "test": "jest --colors",
    "test:coverage": "yarn test --collectCoverage"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "bunyan": "^1.8.14",
    "demux": "https://github.com/lecle/demux-js.git#develop",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6"
  }
}
